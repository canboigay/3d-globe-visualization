{
  "name": "3d-globe-visualization",
  "version": "2.0.0",
  "description": "Interactive 3D globe visualization for threat intelligence, geospatial data, and real-time monitoring. Features multiple textures, tactical overlays, time-series playback, and data clustering.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react --external react-dom --external globe.gl --external three",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "globe",
    "3d",
    "visualization",
    "threat-intelligence",
    "cybersecurity",
    "geospatial",
    "three.js",
    "globe.gl",
    "react",
    "dashboard",
    "real-time",
    "deception",
    "war-game",
    "tactical",
    "starfield",
    "map"
  ],
  "author": "canboigay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/canboigay/3d-globe-visualization"
  },
  "homepage": "https://github.com/canboigay/3d-globe-visualization#readme",
  "bugs": {
    "url": "https://github.com/canboigay/3d-globe-visualization/issues"
  },
  "peerDependencies": {
    "globe.gl": ">=2.27.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.160.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "globe.gl": "^2.27.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
